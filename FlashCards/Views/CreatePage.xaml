<Page
    x:Class="FlashCards.Views.CreatePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:l="using:WinUI3Localizer"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid x:Name="ContentArea">
        <SelectorBar>
            <SelectorBarItem
                l:Uids.Uid="Create_SelectorBar_Front"
                IsSelected="True"
                Style="{StaticResource SubtitleSelectorBarItemStyle}"/>
            <SelectorBarItem l:Uids.Uid="Create_SelectorBar_Back" Style="{StaticResource SubtitleSelectorBarItemStyle}"/>
        </SelectorBar>
    </Grid>
</Page>
